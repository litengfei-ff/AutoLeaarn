{
  "name": "自动学习",
  "manifest_version": 2,
  "version": "1.0",
  "author": "litengfei",
  "description": "自动学习，屏蔽验证码。",
  "browser_action": {
     "default_icon": "icon.png", 
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "learning.js"
      ]
    }
  ],
  "permissions": [
    "*://*/*",
    "tabs",
    "activeTab",
    "notifications"
  ],
  "background":{
     "scripts": ["background.js"]
  }
}